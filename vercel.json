{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "SECRET_ID": "@secret_id",
    "SECRET_KEY": "@secret_key",
    "APP_ID": "@app_id",
    "VUE_APP_KEY": "@vue_app_key"
  }
} 