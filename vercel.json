{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "SECRET_ID": "${SECRET_ID}",
    "SECRET_KEY": "${SECRET_KEY}",
    "APP_ID": "${APP_ID}",
    "VUE_APP_KEY": "${VUE_APP_KEY}"
  }
} 